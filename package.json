{
  "name": "flexspotff-com-d1f3",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "build:all": "run-s build build:bot",
    "build:bot": "tsup-node --config ./bot/tsup.config.ts",
    "db:migrate": "run-p db:migrate:*",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- npx prisma db push --accept-data-loss",
    "db-sync": "scripts/db-sync.sh",
    "dev": "remix vite:dev",
    "dev:all": "run-p dev dev:bot",
    "dev:bot": "tsx watch bot/server.ts",
    "docker": "docker-compose up -d",
    "format": "prettier --write . --check '!./postgres-data'",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "postinstall": "patch-package",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "remix-serve ./build/server/index.js",
    "start:bot": "node ./build/bot/server.cjs",
    "start:production": "prisma migrate deploy && run-p start start:bot",
    "test": "run-s db:migrate:test test:unit",
    "test:unit": "dotenv -e .env.test -- vitest --no-threads --coverage",
    "test:e2e:ci": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.142.0",
    "@aws-sdk/lib-storage": "^3.142.0",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^3.14.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-tabs": "^1.1.0",
    "@remix-run/node": "2.12.1",
    "@remix-run/react": "2.12.1",
    "@remix-run/serve": "2.12.1",
    "bcryptjs": "^2.4.3",
    "bree": "^9.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "discord.js": "^14.16.3",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "isbot": "^4",
    "luxon": "^3.0.1",
    "morgan": "^1.10.0",
    "music-metadata": "^8.0.1",
    "podcast": "^2.0.0",
    "react": "^18.3.1",
