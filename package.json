    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.13",
    "@types/luxon": "^3.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^22.5.5",
