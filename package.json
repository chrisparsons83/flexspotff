    "remix-auth": "^3.2.2",
    "remix-auth-socials": "^1.0.2",
    "remix-typedjson": "^0.4.1",
    "superjson": "^1.9.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
