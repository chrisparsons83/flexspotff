{
  "name": "flexspotff-com-d1f3",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:bree-jobs": "esbuild --platform=node --format=cjs ./express/jobs/*.ts --outdir=build/jobs --bundle",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./express/server.ts --outdir=build",
    "db:migrate": "run-p db:migrate:*",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- npx prisma db push --accept-data-loss",
    "dev": "run-p dev:*",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "dev:bree-jobs": "cross-env NODE_ENV=development npm run build:bree-jobs -- --watch",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "run-s db:migrate:test test:unit",
    "test:unit": "dotenv -e .env.test -- vitest --no-threads --coverage",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {
    "importOrder": [
      "^~/models/(.*)$",
      "^~/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.142.0",
    "@aws-sdk/lib-storage": "^3.142.0",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^3.14.0",
    "@remix-run/express": "^1.6.5",
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "@sentry/remix": "^7.10.0",
    "bcryptjs": "^2.4.3",
    "bree": "^9.1.2",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "luxon": "^3.0.1",
    "morgan": "^1.10.0",
    "music-metadata": "^8.0.1",
    "podcast": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix-auth": "^3.2.2",
    "remix-auth-socials": "^1.0.2",
    "superjson": "^1.9.1",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@remix-run/dev": "^1.6.5",
    "@remix-run/eslint-config": "^1.6.5",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.2",
    "@types/express": "^4.17.13",
    "@types/luxon": "^3.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.35",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "cypress": "^9.6.1",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "esbuild": "^0.14.39",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^2.55.0",
    "msw": "^0.40.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "prisma": "^3.14.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.8"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
